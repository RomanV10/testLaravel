run composer require "roman/testLaravel @dev"

To "providers" in /config/app.php add
    Collective\Html\HtmlServiceProvider::class,
    TestLaravel\Items\ItemsServiceProvide::class,

To "aliases" in "/config/app.php" add
    'Form' => Collective\Html\FormFacade::class,
    'Html' => Collective\Html\HtmlFacade::class,

To composer.json in "autoload-dev:psr-4" add "TestLaravel\\Items\\": "vendor/roman/testLaravel/items/src"

run php artisan vendor:publish
run php artisan migrate

